((let print
   (lambda '(world rows)
     (block
       '((let sink (DataSink 'result))
         (let options '('('type) '('autoref) '('unordered)))
         (let world (ResFill! world sink (Key) rows options '"pure"))
         (return (Commit! world sink))))))

 (let $my_variable (String '1))
 (let $another_var (Int32 '1))

 (let result
   (block
     '((let filtered
         (SelectRange
           (DataSource '"//home/table")
           '('"column1" '"column2")
           (Filter (== (Member '"column1") $my_variable))))
       (return filtered))))

 ()

 (let local (AsList
              (AsStruct
                '('"var1" $my_variable)
                '('"var2" $another_var))))

 (let world (Apply print world local))
 (return world))

{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "yqls",
      "camelcase": "Yqls",
      "title": "Yqls",
      "scope": "source.yqls",
      "file-types": [
        "yqls"
      ],
      "injection-regex": "^yqls$",
      "class-name": "TreeSitterYqls"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Yqls grammar for tree-sitter",
    "authors": [
      {
        "name": "vityaman"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-yqls"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}